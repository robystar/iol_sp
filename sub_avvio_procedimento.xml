          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[if context.getItem('allegato_comunicazione_avvio_procedimento',{})=={}:
    return '''<div class="alert alert-info">Allegare la comunicazione per abilitare l'invio della richiesta</div>'''
elif context.getItem('data_comunicazione_avvio_procedimento')!='':
    msg = 'Comunicazione di avvio procedimento inviata in data ' +  DateToString(context.getItem('data_comunicazione_avvio_procedimento'),format="%d/%m/%Y %H:%M")
    if context.getItem('protocollo_avvio_procedimento'):
        msg = msg + ' Protocollo ' + str(context.getItem('protocollo_avvio_procedimento'))
    return '''<div class="alert alert-success"><strong>%s</strong></div>''' %msg
else:
    return '''<div class="alert alert-info">Per inviare nuovamente la comunicazione annullare il campo "Data comunicazione"</div>''']]></Formula>
